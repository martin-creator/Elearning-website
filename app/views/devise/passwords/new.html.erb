<div class="row">
  <div class="col m6 offset-m3 center">
    <h5>Forgot your password?</h5>
    <%= render partial: "shared/devisemes" %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <div class="input-field">
      <%= f.email_field :email, :id => "email", autofocus: true %>
    </div>
    <button class="btn waves-effect waves-light" type="submit" name="action">Send me reset password instructions</button>
  <% end %>
</div>
</div>
