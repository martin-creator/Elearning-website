<div class="row">
<div class="col m6 offset-m3 center">

<h5>Login</h5>

<%= link_to "Login with  Google", user_google_oauth2_omniauth_authorize_path(:google_oauth2), class: "btn red darken-1 oauth-link", method: :post %> <br /> <br/>
<%= link_to "Sign up with  Github", user_github_omniauth_authorize_path(:github), class: "btn grey darken-2 ", method: :post  %> <br/>



<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
   <%=  render partial: "shared/devisemes" %>

  <div class="input-field">
    <%= f.email_field :email, :class => "validate", :placeholder => "Email" %>
  </div>

  <div class="input-field">
    <%= f.password_field :password, autocomplete: "new-password",  :class => "validate", :placeholder => "Password" %>
  </div>

  <button class ="btn waves-effect waves-light" type="submit" name="action">Login</button>

  <%= link_to "Forgot password", new_user_password_path, :class => "btn waves-effect waves-light" %>

<% end %>

</div>
</div>
